<?xml version="1.0"?>
<launch>
    <arg name="quad_name"/>

    <group ns="$(arg quad_name)">
	    <node name="ros_to_sbus" pkg="agiros" type="ros_to_sbus_bridge" output="screen">
            <param name="sbus_config" value="$(find agilib)/params/volley_sbus.yaml"/>
            <param name="quadrotor" value="$(find agiros)/parameters/quads/volley_drone.yaml"/>
        </node>
    </group>
</launch>
